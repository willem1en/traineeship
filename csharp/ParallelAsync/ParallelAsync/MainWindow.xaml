<Window x:Class="ParallelAsync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParallelAsync"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <StackPanel>
        <Button Content="Lambda expressions" Click="Lambda"/>
        <Button Content="Parallel functions" Click="ParallelF"/>
        <Button Content="Compute squares" Click="ComputeSquares"/>
